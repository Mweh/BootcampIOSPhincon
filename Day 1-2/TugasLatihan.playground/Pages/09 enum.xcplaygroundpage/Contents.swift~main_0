// enumuretion is safe to use for give a fixed value
// avoid things that aren't in the value

enum linkPower {
    case stopTime, fuse, ultrahand
    case rewind, blueprintMemory
}

var linkPowers = linkPower.fuse
linkPowers = .blueprintMemory
linkPowers = .rewind
linkPowers = .ultrahand
let linkPowers4 = linkPower.stopTime
print(linkPowers)
print(linkPowers4)

