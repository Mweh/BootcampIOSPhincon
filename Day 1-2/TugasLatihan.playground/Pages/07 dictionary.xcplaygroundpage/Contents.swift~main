import Foundation

var greeting = "Hello, playground"

//: [Next](@next)

// dict is like array but have unique key
// dict have no particular order
// add default value, otherwise dict will sent optional
// optional means there are nil value chance

var animeRival = [String: String]()
animeRival["Naruto"] = "Sasuke"
animeRival["Goku"] = "Vegeta"
animeRival["Zoro"] = "Sanji"
animeRival["Tanjiro"] = "Inosuke"

print(animeRival)
print(animeRival["Tanjiro", default: "Unknown"])

animeRival["Tanjiro"] = "Zenitsu"
print(animeRival)
print(animeRival["Tanjiro", default: "Unknown"])
print(animeRival["Saitama", default: "Unknown"])


